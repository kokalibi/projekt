--
-- Table structure for table `rendelesek`
--

DROP TABLE IF EXISTS `rendelesek`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!50503 SET character_set_client = utf8mb4 */;
CREATE TABLE `rendelesek` (
  `rendeles_id` int NOT NULL AUTO_INCREMENT,
  `vasarlo_id` int NOT NULL,
  `mennyiseg` int DEFAULT '1',
  `rendeles_datum` date DEFAULT (curdate()),
  `fizetesi_mod` enum('kártya','átutalás','készpénz') COLLATE utf8mb4_hungarian_ci DEFAULT 'kártya',
  `statusz` enum('függőben','feldolgozva','kiszállítva','teljesítve') COLLATE utf8mb4_hungarian_ci DEFAULT 'függőben',
  PRIMARY KEY (`rendeles_id`),
  KEY `vasarlo_id` (`vasarlo_id`),
  CONSTRAINT `rendelesek_ibfk_1` FOREIGN KEY (`vasarlo_id`) REFERENCES `vasarlok` (`vasarlo_id`)
) ENGINE=InnoDB AUTO_INCREMENT=5 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_hungarian_ci;
